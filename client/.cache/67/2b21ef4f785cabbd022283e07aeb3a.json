olumn":0},"generated":{"line":88,"column":0}},{"source":"component/add_task.vue","name":null,"original":{"line":28,"column":0},"generated":{"line":89,"column":0}},{"source":"component/add_task.vue","name":null,"original":{"line":20,"column":0},"generated":{"line":90,"column":0}}],"sources":{"component/add_task.vue":"<template>\n  <div class=\"task-container\">\n    <div class=\"task\">\n      <textarea v-model=\"newTask\" placeholder=\"Write your task here\"></textarea>\n      <div class=\"action\">\n        <a href @click.prevent=\"cancel\">\n          <i class=\"fa fa-arrow-left\"></i> cancel\n        </a>\n        <a href @click.prevent=\"postTask\">\n          <i class=\"fa fa-check\"></i>\n          add\n        </a>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  props: [\"catid\", \"projectid\"],\n  data: function() {\n    return {\n      newTask: \"\",\n      endpoint_task: \"https://api-kanban.herokuapp.com/task\"\n    };\n  },\n  methods: {\n    cancel: function() {\n      this.$emit(\"hide\");\n    },\n    postTask: function() {\n      const data = {\n        categoryId: this.catid,\n        projectId: this.projectid,\n        task: this.newTask\n      };\n      this.request({ method: \"post\", data })\n        .then(res => {\n          this.$emit(\"hide\");\n          this.$emit(\"load-data\");\n        })\n        .catch(err => {\n          err = err.response.data.message;\n          this.alert({\n            title: \"Failed\",\n            icon: \"warning\",\n            msg: err\n          });\n        });\n    },\n    alert: function({ title, icon, msg }) {\n      return Swal.fire({\n        title: title,\n        icon: icon,\n        text: msg\n      });\n    },\n    request: function(params) {\n      const { method, data } = params;\n      const url = this.endpoint_task;\n      return axios({\n        method: method,\n        url: url,\n        data,\n        headers: {\n          token: localStorage.token\n        },\n      });\n    }\n  }\n};\n</script>"},"lineCount":null}},"error":null,"hash":"88aee35501d4d33d580e2fbd3b2f7d67","cacheData":{"env":{}}}
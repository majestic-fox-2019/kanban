ht: 500;\n}\n\n.button-secondary{\n  color: #ffffff;\n  background-color: rgba($color: #ffffff, $alpha: .5);\n\n  &:hover{\n    color: #ffffff;\n    background-color: rgba($color: #ffffff, $alpha: .65);\n    cursor: pointer;\n  }\n}\n\n.button-primary{\n  color: #ffffff;\n  background-color: rgba($color: #042549, $alpha: 1);\n\n  &:hover{\n    color: #ffffff;\n    background-color: rgba($color: #041e3a, $alpha: 1);\n    cursor: pointer;\n  }\n}","src/assets/scss/custom/_space.scss":".no-p{\n  padding: 0 !important;\n}\n\n.no-m{\n  margin: 0 !important;\n}","src/assets/scss/custom/_custom.scss":"body {\n  background-color: #ffffff;\n  height: 100%;\n  margin: 0;\n  padding: 0;\n}\n\n.container{\n  max-width: 1280px;\n}\n\n.bg-special{\n  background-color: #042549;\n}\n\n.login-page{\n  display: none;\n  margin-top: 8rem;\n}\n\n.main-page{\n  display: none;\n}\n\n.bold{\n  font-weight: bold !important;\n}\n\n.navbar-right{\n  display: flex;\n\n    & a img{\n      height: 35px;\n      border-radius: 100%;\n      margin-left: 10px;\n    }\n\n    & .toggle-user{\n      text-decoration: none;\n\n      &:after{\n        vertical-align: .15rem !important;\n        color: #ffffff !important;\n      }\n\n      & .users{\n        color: rgba($color: #ffffff, $alpha: .7);\n        padding-right: 0.5rem;\n\n        &:hover{\n          color: rgba($color: #ffffff, $alpha: 1.0);\n        }\n      }\n    }\n\n    & .user-div-menu{\n      margin: 0.5rem 0 0;\n      \n      & .logout{\n        &:hover{\n          cursor: pointer;\n        }\n      }\n    }\n}\n\n.main{\n  height: 100%;\n  padding: 1rem .8rem;\n\n  & .boards{\n    display: flex;\n\n    & .category + .category{\n      margin: 0 0 0 1rem;\n    }\n\n    & .category{\n      width: 350px;\n      height: fit-content;\n      background-color: #ebecf0;\n\n      & .card-board-header{\n        background-color: #042549;\n        color: #ffffff;\n        padding: 10px 20px; \n        display: flex;\n        justify-content: space-between;\n\n        & span{\n          font-weight: bold;\n        }\n\n        & a i{\n          color: #ffffff;\n\n          &:hover{\n            cursor: pointer;\n            color: #cfcfcf;\n          }\n        }\n      }\n\n      & .card-board-content{\n        padding: 10px;\n        max-height: 480px;\n        height: auto;\n        overflow-y: auto;\n\n        & .content-board{\n          background-color: #ffffff;\n          border-radius: 5px;\n          padding: 10px;\n          margin-bottom: 10px;\n          \n          & .content-task{\n            & p{\n              white-space: normal;\n              margin-bottom: 0 !important;\n            }\n          }\n\n          & .detail-task{\n            margin-top: 5px;\n            display: flex;\n            justify-content: space-between;\n\n            & .detail-task-left{\n              margin-top: 5px;\n\n              & p{\n                font-size: 10px;\n                color: rgba($color: #000000, $alpha: .5);\n                margin-bottom: 0 !important;\n              }\n            }\n\n            & .detail-task-right{\n              display:none;\n\n              & span.icon-act{\n                font-size: .85rem;\n                margin-left: 5px;\n                color: rgba($color: #000000, $alpha: .5);\n                margin-bottom: 0 !important;\n\n                &:hover{\n                  cursor: pointer;\n                  color: rgba($color: #000000, $alpha: 1);\n                }\n\n                &::after{\n                  display: none;\n                }\n              }\n\n              & .user-div-menu{\n                & .move{\n                  font-size: .85rem;\n                  font-weight: 500 !important;\n                  color: rgba($color: #000000, $alpha: .5);\n                  margin-bottom: 0 !important;\n\n                  &:hover{\n                    cursor: pointer;\n                  }\n                }\n              }\n            }\n          }\n\n          &:hover .detail-task-right{\n            display: block;\n          }\n\n        }\n      }\n\n      & .card-board-add-task span{\n        color: rgba($color: #000000, $alpha: .5);\n        &:hover{\n          cursor: pointer;\n          color: rgba($color: #000000, $alpha: 1.0);\n        }\n      }\n    }\n  }\n}\n\n.textarea.list-card-composer-textarea, \n.textarea.list-card-composer-textarea:focus{\n  background: none;\n  border: none;\n  box-shadow: none;\n  height: auto;\n  margin-bottom: 4px;\n  max-height: 80px;\n  overflow-y: auto;\n  padding: 0;\n}\n\n.form-signin-regist{\n  width: 100%;\n  max-width: 420px;\n  padding: 15px;\n  margin: auto;\n}\n\n.bg-front{\n  background-color: rgba($color: #000000, $alpha: .1);\n}\n\n.special .active a,\n.special .active a:hover {\n  background-color: #042549 !important;\n}\n\na.nav-link{\n  color: #042549 !important;\n}\n\na.nav-link.active{\n  color: #ffffff !important;\n}\n\n.nav-pills .nav-link.active, .nav-pills .show>.nav-link{\n  background-color: #042549 !important;\n}\n\n.card-task-footer{\n  padding: .75rem 10px;\n  background-color: rgba(0,0,0,.03);\n  border-top: 1px solid rgba(0,0,0,.125);\n}\n\n.my-board{\n  background-color: #fffddb !important;\n}\n\n.abcRioButton{\n  margin: 0 auto;\n}\n\n.login-google{\n  width: 100%;\n  text-align: center;\n}\n.g-sign-in-button {\n  margin: 10px;\n  display: inline-block;\n  width: 240px;\n  height: 50px;\n  background-color: #4285f4;\n  color: #fff;\n  border-radius: 1px;\n  box-shadow: 0 2px 4px 0 rgba(0,0,0,.25);\n  transition: background-color .218s, border-color .218s, box-shadow .218s;\n}\n.g-sign-in-button:hover {\n  cursor: pointer;\n  -webkit-box-shadow: 0 0 3px 3px rgba(66,133,244,.3);\n  box-shadow: 0 0 3px 3px rgba(66,133,244,.3);\n}\n.g-sign-in-button:active {\n  background-color: #3367D6;\n  transition: background-color 0.2s;\n}\n.g-sign-in-button .content-wrapper {\n  height: 100%;\n  width: 100%;\n  border: 1px solid transparent;\n}\n.g-sign-in-button img {\n  width: 18px;\n  height: 18px;\n  margin-bottom: 1rem;\n}\n.g-sign-in-button .logo-wrapper {\n   padding: 15px;\n   background:#fff;\n   width: 54px;\n   height: 100%;\n   border-radius: 1px; \n   display: inline-block;\n}\n.g-sign-in-button .text-container {\n    font-family: Roboto,arial,sans-serif;\n    font-weight: 500;\n    letter-spacing: .21px;\n    font-size: 16px;\n    line-height: 48px;\n    vertical-align: top;\n    border: none;\n    display: inline-block;\n    text-align: center;\n    width: 180px;\n}"},"lineCount":null}},"error":null,"hash":"ac413d215ffe8e53490f5c0756071cbe","cacheData":{"env":{}}}